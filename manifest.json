{
  "name": "Chronus Mini",
  "description" : "Chronus Extension",
  "version": "3.0",
  "browser_action": {
    "default_icon": {
      "16": "images/chronus_color_16.png",      
      "48": "images/chronus_color_48.png",
      "128": "images/chronus_color_128.png"
    },
    "default_popup": "popup.html"
